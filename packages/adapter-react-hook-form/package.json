{
  "name": "@next-cool-action/adapter-react-hook-form",
  "version": "0.0.2",
  "private": false,
  "description": "This adapter offers a way to seamlessly integrate next-cool-action with react-hook-form.",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "./hooks": {
      "types": "./dist/hooks.d.mts",
      "default": "./dist/hooks.mjs"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/index.d.mts"
      ],
      "hooks": [
        "./dist/hooks.d.mts"
      ]
    }
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "tsc && prettier --write . && eslint .",
    "build": "tsdown"
  },
  "keywords": [
    "next",
    "nextjs",
    "react",
    "rsc",
    "react server components",
    "mutation",
    "action",
    "actions",
    "react actions",
    "next actions",
    "server actions",
    "next-cool-action",
    "next cool action",
    "react-hook-form",
    "react hook form",
    "react forms"
  ],
  "author": "Bhargav Ponnapalli",
  "license": "MIT",
  "engines": {
    "node": ">=18.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@hookform/resolvers": "^5.2.2",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "catalog:",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "next-cool-action": "workspace:*",
    "prettier": "^3.4.2",
    "react": "catalog:",
    "react-hook-form": "^7.55.0",
    "tsdown": "^0.14.1",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "typescript-eslint": "^8.22.0",
    "zod": "^4.1.13"
  },
  "peerDependencies": {
    "@hookform/resolvers": ">= 5.0.0",
    "next": ">= 14.0.0",
    "next-cool-action": "^0.0.5",
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0",
    "react-hook-form": ">= 7.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/imbhargav5/next-cool-action.git"
  },
  "homepage": "https://next-cool-action-docs.vercel.app",
  "publishConfig": {
    "access": "public"
  }
}
